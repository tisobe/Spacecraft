sed -e 's/ACIS-I/ACIS-S/g' acisi_rnd.html > x1.html
sed -e 's/ACIS-I/HRC-I/g' acisi_rnd.html > x2.html
sed -e 's/ACIS-I/HRC-S/g' acisi_rnd.html > x3.html
sed -e 's/ACIS-I/TOTAL/g' acisi_rnd.html > x4.html
sed -e 's/ai_/as_/g' x1.html > aciss_rnd.html
sed -e 's/ai_/hi_/g' x2.html > hrci_rnd.html
sed -e 's/ai_/hs_/g' x3.html > hrcs_rnd.html
sed -e 's/ai_/tot_/g' x4.html > tot_rnd.html
/usr/bin/rm x*html
